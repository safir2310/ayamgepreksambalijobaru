# Auto detect text files and perform LF normalization
* text=auto

# Force LF for specific file types
*.tsx text eol=lf
*.ts text eol=lf
*.tsx text eol=lf
*.json text eol=lf
*.md text eol=lf
*.css text eol=lf

# Denoise
package-lock.json linguist-generated=false
yarn.lock linguist-generated=false
pnpm-lock.yaml linguist-generated=false
